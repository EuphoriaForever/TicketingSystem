          <!--This is where we will keep track of the events offered and the collabs-->
          <div class="row">
            <div class="col-lg-6 col-md-12">
              <div class="card">
                <div class="card-header card-header-primary">
                  <h4 class="card-title">Employees Stats</h4>
                  <p class="card-category">New employees on 15th September, 2016</p>
                </div>
                <div class="card-body table-responsive">
                  <table class="table table-hover">
                    <thead class="text-warning">
                      <th>Event Name</th>
                      <th>Venue</th>
                      <th>Date and Time</th>
                      <th>Sales</th>
                    </thead>
                    <tbody>
                      <% if(events.length == 0){%>
                          <tr><td>No events yet</td></tr>
                     <% }else{%>
                          <% for(var x =0; x < events.length && x < 4 ; x++){ %>
                          <tr>
                            <td><%= events[x].eventName %></td>
                            <td><%= events[x].eventVenue %></td>
                            <td><%= events[x].eventDateTime %></td>
                            <td>Php.<%= events[x].total_Sales %></td>
                          </tr>
                          <% } %>
                        <% } %>
                    </tbody>
                  </table>
                  <a class="btn btn-primary pull-left" href="/adminEvent">See more</a>
                </div>
              </div>
            </div>
            <div class="col-lg-6 col-md-12">
               <div class="card">
                    <div class="card-header card-header-tabs card-header-warning">
                      <div class="nav-tabs-navigation">
                        <div class="nav-tabs-wrapper">
                          <span class="nav-tabs-title">Emails on:</span>
                          <ul class="nav nav-tabs" data-tabs="tabs">
                            <li class="nav-item">
                              <a class="nav-link" href="#settings" data-toggle="tab">
                                <i class="material-icons">cloud</i> Collabs
                                <div class="ripple-container"></div>
                              </a>
                            </li>
                          </ul>
                        </div>
                      </div>
                    </div>
                    <div class="card-body">
                      <div class="tab-content">   
                        <div class="tab-pane active" id="settings">
                          <table class="table">
                            <thead class="text-warning">
                              <th>From</th>
                              <th>Message</th>
                              <th>Email</th>
                            </thead>
                            <tbody>
                              <tr>

                              <% if(events.length == 0){%>
                                    <tr><td>No Messages Yet</td></tr>
                              <% }else{%>
                                    <% for(var a =0 ; a < collabs.length && a < 4 ; a++){ %>
                                      <tr>
                                        <td><%= collabs[a].fullName %></td>
                                        <td><%= collabs[a].message %></td>
                                        <td><%= collabs[a].email %></td>
                                      </tr>
                                    <% } %>
                              <% } %>
                              <!--LOOP FOR THE ROWS WILL START HERE-->
                            </tbody>
                          </table>
                          <a class="btn btn-warning pull-left" href="/adminCollab">See more</a>
                        </div>
