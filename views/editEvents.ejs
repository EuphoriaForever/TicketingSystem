  <div class="content">
    <div class="container-fluid">
      <div class="row">
    
        <div class="col-md-12">
          <div class="card card-plain">
            <div class="card-header card-header-primary">
              <h4 class="card-title mt-0"> Add another event</h4>
              <p class="card-category"> Go ahead! Make your day!</p>
            </div>
            <div class="card-body">
              
                <form action="/editEvent" method="POST">
                  <div class="row">
                
                   <div class="col-md-3">
                      <div class="form-group">
                        <label class="bmd-label-floating">Event Name</label>
                        <input type="text" name="eventName" class="form-control" value="<%= rows[0].eventName %>">
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="form-group">
                        <label class="bmd-label-floating">Event Date</label>
                        <input type="datetime-local" class="form-control"  name="eventDateTime" value="<%= rows[0].eventDateTime %>"
                        min="2020-09-27T00:00" max="2020-10-14T00:00">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-4">
                      <div class="form-group">
                        <label class="bmd-label-floating">Price in Php.</label>
                        <input type="number" class="form-control" name="eventPrice" value="<%= rows[0].eventPrice %>">
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="form-group">
                        <label class="bmd-label-floating">Capacity of the event</label>
                        <input type="number" class="form-control" name="num_Tickets" value="<%= rows[0].num_Tickets %>">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-10">
                      <div class="form-group">
                        <label class="bmd-label-floating">Venue</label>
                        <input type="text" class="form-control" name="eventVenue" value="<%= rows[0].eventVenue %>">
                      </div>
                    </div>
                  </div>
                
                  <div class="row">
                    <div class="col-md-10">
                      <div class="form-group">
                        <div class="form-group">
                          <label class="bmd-label-floating">Event Description</label>
                          <textarea class="form-control" rows="5" name="descript" value="<%= rows[0].descript %>"></textarea>
                        </div>
                      </div>
                    </div>
                  </div>
                  <input type="hidden" name="event_ID" value="<%= rows[0].event_ID %>">
                  <button type="submit" class="btn btn-primary pull-right">Submit</button>
                  <div class="clearfix"></div>
                 
                </form>
              
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
